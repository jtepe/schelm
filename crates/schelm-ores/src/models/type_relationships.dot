</TABLE>>];

        legend_streaming [label=<
<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" COLOR="#D1D5DB">
<TR><TD BGCOLOR="#0891B2" CELLPADDING="6" WIDTH="180" ALIGN="CENTER"><FONT COLOR="white" POINT-SIZE="10"><B>Streaming</B></FONT></TD></TR>
<TR><TD BGCOLOR="white" CELLPADDING="4" ALIGN="LEFT"><FONT POINT-SIZE="9" COLOR="#4B5563">SSE streaming event types</FONT></TD></TR>
</TABLE>>];

        legend_other [label=<
<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" COLOR="#D1D5DB">
<TR><TD BGCOLOR="#6B7280" CELLPADDING="6" WIDTH="180" ALIGN="CENTER"><FONT COLOR="white" POINT-SIZE="10"><B>Type Alias / Other</B></FONT></TD></TR>
<TR><TD BGCOLOR="white" CELLPADDING="4" ALIGN="LEFT"><FONT POINT-SIZE="9" COLOR="#4B5563">Aliases and marker types</FONT></TD></TR>
</TABLE>>];

        legend_solid_edge [label=<
<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="0">
<TR><TD CELLPADDING="4" ALIGN="LEFT"><FONT POINT-SIZE="9" COLOR="#4B5563">Solid line: contains / wraps type</FONT></TD></TR>
</TABLE>>];

        legend_dashed_edge [label=<
<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="0">
<TR><TD CELLPADDING="4" ALIGN="LEFT"><FONT POINT-SIZE="9" COLOR="#4B5563">Dashed line: references enum value</FONT></TD></TR>
</TABLE>>];

        // Stack legend items vertically
        legend_api -> legend_struct [style=invis];
        legend_struct -> legend_value_enum [style=invis];
        legend_value_enum -> legend_tagged_enum [style=invis];
        legend_tagged_enum -> legend_streaming [style=invis];
        legend_streaming -> legend_other [style=invis];
        legend_other -> legend_solid_edge [style=invis];
        legend_solid_edge -> legend_dashed_edge [style=invis];
    }
}
