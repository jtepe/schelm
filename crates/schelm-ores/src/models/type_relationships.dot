// Type Relationship Diagram for schelm-ores models
// Generate SVG: dot -Tsvg type_relationships.dot -o type_relationships.svg
// If rendering is slow, change splines=ortho to splines=true

digraph TypeRelationships {
    graph [
        rankdir=TB
        fontname="Helvetica"
        bgcolor="white"
        pad="0.5"
        nodesep=0.7
        ranksep=1.0
        newrank=true
        splines=ortho
    ];

    node [
        shape=none
        fontname="Helvetica"
        margin="0"
    ];

    edge [
        fontname="Helvetica"
        fontsize=9
        color="#475569"
        arrowsize=0.7
    ];

