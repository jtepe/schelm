<TR><TD BGCOLOR="white" CELLPADDING="4" ALIGN="LEFT" BALIGN="LEFT"><FONT FACE="Consolas,monospace" POINT-SIZE="9" COLOR="#4B5563">
token : String<BR ALIGN="LEFT"/>
logprob : f64<BR ALIGN="LEFT"/>
bytes : [i32]<BR ALIGN="LEFT"/>
top_logprobs : [TopLogProb]<BR ALIGN="LEFT"/>
</FONT></TD></TR>
</TABLE>>];

    // --- Streaming ---

    ErrorPayload [label=<
<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" COLOR="#D1D5DB">
<TR><TD BGCOLOR="#0891B2" CELLPADDING="6" ALIGN="CENTER"><FONT COLOR="white" POINT-SIZE="11"><B>ErrorPayload</B></FONT></TD></TR>
<TR><TD BGCOLOR="white" CELLPADDING="4" ALIGN="LEFT" BALIGN="LEFT"><FONT FACE="Consolas,monospace" POINT-SIZE="9" COLOR="#4B5563">
ty : String<BR ALIGN="LEFT"/>
code : String?<BR ALIGN="LEFT"/>
message : String<BR ALIGN="LEFT"/>
param : String?<BR ALIGN="LEFT"/>
headers : Map&lt;String, String&gt;?<BR ALIGN="LEFT"/>
</FONT></TD></TR>
</TABLE>>];

    // --- Other ---

    EmptyModelParam [label=<
<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" COLOR="#D1D5DB">
<TR><TD BGCOLOR="#6B7280" CELLPADDING="6" ALIGN="CENTER"><FONT COLOR="white" POINT-SIZE="11"><B>EmptyModelParam</B></FONT></TD></TR>
<TR><TD BGCOLOR="white" CELLPADDING="4" ALIGN="LEFT" BALIGN="LEFT"><FONT FACE="Consolas,monospace" POINT-SIZE="9" COLOR="#9CA3AF">
(empty)<BR ALIGN="LEFT"/>
</FONT></TD></TR>
</TABLE>>];

    MetadataParam [label=<
<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" COLOR="#D1D5DB">
<TR><TD BGCOLOR="#6B7280" CELLPADDING="6" ALIGN="CENTER"><FONT COLOR="white" POINT-SIZE="11"><B>MetadataParam</B></FONT><BR/><FONT COLOR="#D1D5DB" POINT-SIZE="8">type alias</FONT></TD></TR>
<TR><TD BGCOLOR="white" CELLPADDING="4" ALIGN="LEFT" BALIGN="LEFT"><FONT FACE="Consolas,monospace" POINT-SIZE="9" COLOR="#4B5563">
= Map&lt;String, String&gt;<BR ALIGN="LEFT"/>
</FONT></TD></TR>
</TABLE>>];

    // ================================================================
    //  EDGES: CreateResponseBody field references
    // ================================================================

    CreateResponseBody -> CreateResponseInput [weight=5];
    CreateResponseBody -> IncludeEnum [style=dashed, color="#94A3B8"];
    CreateResponseBody -> ResponsesToolParam;
    CreateResponseBody -> ToolChoiceParam;
    CreateResponseBody -> MetadataParam [style=dashed, color="#94A3B8"];
    CreateResponseBody -> TextParam;
    CreateResponseBody -> StreamOptionsParam;
    CreateResponseBody -> ReasoningParam;
    CreateResponseBody -> TruncationEnum [style=dashed, color="#94A3B8"];
    CreateResponseBody -> ServiceTierEnum [style=dashed, color="#94A3B8"];

    // ================================================================
    //  EDGES: Input item hierarchy
    // ================================================================

    CreateResponseInput -> ItemParam [weight=5];

    ItemParam -> ItemReferenceParam;
    ItemParam -> ReasoningItemParam;
    ItemParam -> MessageItemParam [weight=3];
    ItemParam -> FunctionCallItemParam;
    ItemParam -> FunctionCallOutputItemParam;

    MessageItemParam -> UserMessageItemParam [weight=3];
    MessageItemParam -> SystemMessageItemParam;
    MessageItemParam -> DeveloperMessageItemParam;
    MessageItemParam -> AssistantMessageItemParam;

    // ================================================================
    //  EDGES: Message content (Param side)
    // ================================================================

    UserMessageItemParam -> UserMessageContent [weight=5];
    UserMessageContent -> UserMessageContentPart [weight=5];
    UserMessageContentPart -> InputTextContentParam;
    UserMessageContentPart -> InputImageContentParamAutoParam;
    UserMessageContentPart -> InputFileContentParam;

    SystemMessageItemParam -> SystemMessageContent [weight=5];
    DeveloperMessageItemParam -> SystemMessageContent;
    SystemMessageContent -> InputTextContentParam;

    AssistantMessageItemParam -> AssistantMessageContent [weight=5];
    AssistantMessageContent -> AssistantMessageContentPart [weight=5];
    AssistantMessageContentPart -> OutputTextContentParam;
    AssistantMessageContentPart -> RefusalContentParam;

    OutputTextContentParam -> UrlCitationParam;

    InputImageContentParamAutoParam -> DetailEnum [style=dashed, color="#94A3B8", constraint=false];

    // ================================================================
    //  EDGES: Reasoning (Param side)
    // ================================================================

    ReasoningItemParam -> ReasoningSummaryContentParam [weight=5];

    // ================================================================
    //  EDGES: Function call (Param side)
    // ================================================================

    FunctionCallItemParam -> FunctionCallStatus [style=dashed, color="#94A3B8", constraint=false];
    FunctionCallOutputItemParam -> FunctionCallOutput [weight=3];
    FunctionCallOutputItemParam -> FunctionCallStatus [style=dashed, color="#94A3B8", constraint=false];

    FunctionCallOutput -> FunctionCallOutputPart [weight=5];
    FunctionCallOutputPart -> InputTextContentParam [constraint=false];
    FunctionCallOutputPart -> InputImageContentParamAutoParam [constraint=false];
    FunctionCallOutputPart -> InputFileContentParam [constraint=false];
    FunctionCallOutputPart -> InputVideoContent;

    // ================================================================
    //  EDGES: Tool configuration
    // ================================================================

    ResponsesToolParam -> FunctionToolParam [weight=5];

    ToolChoiceParam -> SpecificToolChoiceParam;
    ToolChoiceParam -> ToolChoiceValueEnum [style=dashed, color="#94A3B8"];
    ToolChoiceParam -> AllowedToolsParam;

    SpecificToolChoiceParam -> SpecificFunctionParam [weight=5];
    AllowedToolsParam -> SpecificToolChoiceParam;
    AllowedToolsParam -> ToolChoiceValueEnum [style=dashed, color="#94A3B8", constraint=false];

    // ================================================================
    //  EDGES: Request config â†’ enums
    // ================================================================

    TextParam -> VerbosityEnum [style=dashed, color="#94A3B8"];
    ReasoningParam -> ReasoningEffortEnum [style=dashed, color="#94A3B8"];
    ReasoningParam -> ReasoningSummaryEnum [style=dashed, color="#94A3B8"];

    // ================================================================
    //  EDGES: ResponseResource field references
    // ================================================================

    ResponseResource -> IncompleteDetails;
    ResponseResource -> ItemField [weight=5];
    ResponseResource -> Error;
    ResponseResource -> Tool;
    ResponseResource -> TruncationEnum [style=dashed, color="#94A3B8", constraint=false];
    ResponseResource -> TextField;
    ResponseResource -> Usage;
    ResponseResource -> Reasoning;

    // ================================================================
    //  EDGES: Response item hierarchy
    // ================================================================

    ItemField -> Message [weight=3];
    ItemField -> FunctionCall;
    ItemField -> FunctionCallOutputResource;
    ItemField -> ReasoningBody;

    // ================================================================
    //  EDGES: Message (Response side)
    // ================================================================

    Message -> MessageStatus [style=dashed, color="#94A3B8", constraint=false];
    Message -> MessageRole [style=dashed, color="#94A3B8", constraint=false];
    Message -> MessageContentPart [weight=5];

    MessageContentPart -> InputTextContent;
    MessageContentPart -> OutputTextContent [weight=3];
    MessageContentPart -> TextContent;
    MessageContentPart -> SummaryTextContent;
    MessageContentPart -> ReasoningTextContent;
    MessageContentPart -> RefusalContent;
    MessageContentPart -> InputImageContent;
    MessageContentPart -> InputFileContent;
    MessageContentPart -> InputVideoContent [constraint=false];

    // ================================================================
    //  EDGES: Response content details
    // ================================================================

    OutputTextContent -> Annotation;
    OutputTextContent -> LogProb;

    Annotation -> UrlCitationBody [weight=5];
    LogProb -> TopLogProb [weight=5];

    InputImageContent -> DetailEnum [style=dashed, color="#94A3B8", constraint=false];

    // ================================================================
    //  EDGES: Function call (Response side)
    // ================================================================

    FunctionCall -> FunctionCallStatus [style=dashed, color="#94A3B8", constraint=false];
    FunctionCallOutputResource -> FunctionCallOutput [constraint=false];
    FunctionCallOutputResource -> FunctionCallOutputStatusEnum [style=dashed, color="#94A3B8", constraint=false];

    // ================================================================
    //  EDGES: ReasoningBody
    // ================================================================

    ReasoningBody -> MessageContentPart [constraint=false];

    // ================================================================
    //  EDGES: Response tools & config
    // ================================================================

    Tool -> FunctionTool [weight=5];
    TextField -> VerbosityEnum [style=dashed, color="#94A3B8", constraint=false];
    Usage -> InputTokensDetails [weight=5];
    Usage -> OutputTokensDetails [weight=5];
    Reasoning -> ReasoningEffortEnum [style=dashed, color="#94A3B8", constraint=false];
    Reasoning -> ReasoningSummaryEnum [style=dashed, color="#94A3B8", constraint=false];

    // ================================================================
    //  EDGES: StreamingEvent
    // ================================================================

    StreamingEvent -> ResponseResource [style=dashed, color="#0891B2", constraint=false];
    StreamingEvent -> ItemField [style=dashed, color="#0891B2", constraint=false];
    StreamingEvent -> MessageContentPart [style=dashed, color="#0891B2", constraint=false];
    StreamingEvent -> LogProb [style=dashed, color="#0891B2", constraint=false];
    StreamingEvent -> ErrorPayload [weight=5, color="#0891B2"];

    // ================================================================
    //  LAYOUT CONSTRAINTS
    // ================================================================

    // Top-level API types side by side
    {rank=same; CreateResponseBody; ResponseResource;}

    // Request and response config on same level
    {rank=same; TextParam; StreamOptionsParam; ReasoningParam; TextField; Usage; Reasoning;}

    // Item-level enums side by side
    {rank=same; ItemParam; ItemField;}

    // Message item params and response message items aligned
    {rank=same; MessageItemParam; Message; FunctionCall; FunctionCallOutputResource; ReasoningBody;}

    // Simple value enums grouped together
    {rank=same; DetailEnum; MessageRole; MessageStatus; FunctionCallStatus; FunctionCallOutputStatusEnum;}
    {rank=same; VerbosityEnum; ReasoningEffortEnum; ReasoningSummaryEnum; TruncationEnum; ServiceTierEnum; IncludeEnum; ToolChoiceValueEnum;}

    // Content part enums aligned
    {rank=same; UserMessageContentPart; AssistantMessageContentPart; MessageContentPart;}

    // Leaf content types (Param)
    {rank=same; InputTextContentParam; InputImageContentParamAutoParam; InputFileContentParam; OutputTextContentParam; RefusalContentParam;}

    // Leaf content types (Response)
    {rank=same; InputTextContent; OutputTextContent; TextContent; SummaryTextContent; ReasoningTextContent; RefusalContent; InputImageContent; InputFileContent;}

    // Annotations and logprobs
    {rank=same; Annotation; LogProb; UrlCitationParam;}
    {rank=same; UrlCitationBody; TopLogProb;}

    // Streaming at the bottom
    {rank=same; StreamingEvent; ErrorPayload;}

    // Token details together
    {rank=same; InputTokensDetails; OutputTokensDetails;}

    // Tool config hierarchy
    {rank=same; ToolChoiceParam; ResponsesToolParam;}
    {rank=same; SpecificToolChoiceParam; AllowedToolsParam; FunctionToolParam;}
    {rank=same; SpecificFunctionParam; EmptyModelParam;}

