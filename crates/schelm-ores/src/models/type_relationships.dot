            fontname="Helvetica"
            fontsize=14
            fontcolor="#1F2937"
            style="rounded,dashed"
            color="#9CA3AF"
            bgcolor="#F9FAFB"
            labeljust=l
            margin=16
        ];

        legend_api [label=<
<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" COLOR="#D1D5DB">
<TR><TD BGCOLOR="#DC2626" CELLPADDING="6" WIDTH="180" ALIGN="CENTER"><FONT COLOR="white" POINT-SIZE="10"><B>API Entry Point</B></FONT></TD></TR>
<TR><TD BGCOLOR="white" CELLPADDING="4" ALIGN="LEFT"><FONT POINT-SIZE="9" COLOR="#4B5563">CreateResponseBody, ResponseResource</FONT></TD></TR>
</TABLE>>];

        legend_struct [label=<
<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" COLOR="#D1D5DB">
